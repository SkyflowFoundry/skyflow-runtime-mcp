# Optional fallback configuration
# These values are used if not provided as query parameters in the request
# Most deployments will pass these via query parameters instead

VAULT_URL=""
VAULT_ID=""
WORKSPACE_ID=""
ACCOUNT_ID=""

# Note: SKYFLOW_API_KEY and REQUIRED_BEARER_TOKEN are no longer used
# The bearer token is now passed through from the client to Skyflow

# Anonymous Mode Configuration
# When set, allows unauthenticated requests with limited functionality
# Only the 'dehydrate' tool works in anonymous mode (with ENTITY_UNIQUE_COUNTER tokens)
ANON_MODE_API_KEY=""          # Skyflow API key for demo vault
ANON_MODE_VAULT_ID=""         # Demo vault ID
ANON_MODE_VAULT_URL=""        # Demo vault URL (e.g., https://demo.vault.skyflowapis.com)

# Anonymous Mode Rate Limiting
ANON_MODE_RATE_LIMIT_REQUESTS=10    # Max requests per window (default: 10)
ANON_MODE_RATE_LIMIT_WINDOW_MS=60000  # Window duration in milliseconds (default: 1 minute)
